<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_signup"
    app:startDestination="@id/signUpFragment1">
    <fragment
        android:id="@+id/signUpFragment1"
        android:name="com.kunize.uswtimetable.signup.SignUpFragment1"
        android:label="fragment_sign_up1"
        tools:layout="@layout/fragment_sign_up1">
        <action
            android:id="@+id/action_signUpFragment1_to_signUpFragment2"
            app:destination="@id/signUpFragment2" />
    </fragment>
    <fragment
        android:id="@+id/signUpFragment2"
        android:name="com.kunize.uswtimetable.signup.SignUpFragment2"
        android:label="fragment_sign_up2"
        tools:layout="@layout/fragment_sign_up2" >
        <argument android:name="user_id"
            app:argType="string" />
        <argument
            android:name="user_pw"
            app:argType="string" />
        <argument
            android:name="term_checked"
            app:argType="boolean"
            android:defaultValue="false" />
    </fragment>
</navigation>